        "def check_anagram(str1,str2):\n",
        "  str1=str1.replace(\" \",\"\")\n",
        "  str2=str2.replace(\" \",\"\")\n",
        "  if len(str1)==len(str2):\n",
        "    return sorted(str1)==sorted(str2)\n",
        "\n",
        "  return false\n",
        "string1=input(\"s=\")\n",
        "string2=input(\"t=\")\n",
        "if check_anagram(string1,string2):\n",
        "  print(\"True\")\n",
        "else:\n",
        "  print(\"false\")\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
